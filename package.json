{
  "name": "psicologia-vitoria-api",
  "version": "1.0.0",
  "description": "API Project for Psicologia Vitoria",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/server.ts",
    "start:dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start:prod": "node dist/server.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/config/datasource.ts",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/config/datasource.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/config/datasource.ts",
    "migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show -d src/config/datasource.ts",
    "seed": "ts-node src/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.0",
    "googleapis": "^144.0.0",
    "mysql2": "^3.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "typeorm": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.20",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/passport-jwt": "^4.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
